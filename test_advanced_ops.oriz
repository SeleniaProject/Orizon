func main() {
    let a = 10;
    let b = 3;
    
    // ビット演算
    let bit_and = a & b;
    let bit_or = a | b;
    let bit_xor = a ^ b;
    let bit_not = ~a;
    
    // シフト演算
    let left_shift = a << 2;
    let right_shift = a >> 1;
    
    // 複合代入演算
    let c = 5;
    c += 3;
    c -= 1;
    c *= 2;
    
    print("高度な演算子テスト完了");
}
