func main() {
    let a = 10;
    let b = 5;
    let c = 2;
    
    // 複雑な算術式
    let result1 = a + b * c;        // 10 + (5 * 2) = 20
    let result2 = (a + b) * c;      // (10 + 5) * 2 = 30
    let result3 = a - b + c;        // 10 - 5 + 2 = 7
    let result4 = a * b / c;        // 10 * 5 / 2 = 25
    let result5 = a % b + c;        // 10 % 5 + 2 = 2
    
    // 単項演算子
    let neg_a = -a;
    let pos_b = +b;
    
    print("複雑な算術式テスト完了");
}
