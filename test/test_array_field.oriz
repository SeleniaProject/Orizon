func test_array(): int {
    let arr = [1, 2, 3];
    arr[0] = 10;
    arr[1] += 5;
    return arr[0] + arr[1];
}

struct Point {
    x: int,
    y: int
}

func test_field(): int {
    let p = Point{x: 10, y: 20};
    p.x = 30;
    p.y += 15;
    return p.x + p.y;
}

func main(): int {
    let a = test_array();
    let b = test_field();
    return a + b;
}
