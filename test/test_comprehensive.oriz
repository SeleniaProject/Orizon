func main() {
    // 数値リテラル（10進数、16進数、2進数、8進数）
    let dec = 42;
    let hex = 0xFF;
    let bin = 0b1010;
    let oct = 0o755;
    
    // 算術演算
    let sum = dec + hex;
    let product = bin * oct;
    let result = (sum - product) / 2;
    
    // 文字列リテラル
    let message = "計算結果: ";
    
    // 出力
    print(message);
    print(result);
}
