func add(a: i32, b: i32) -> i32 {
    return a + b;
}

func calculate(x: f64, y: f64) -> f64 {
    let result = x * 2.5 + y / 3.14;
    
    if result >= 10.0 {
        return result;
    } else {
        return 0.0;
    }
}

func main() {
    let sum = add(5, 10);
    let calc = calculate(15.5, 9.42);
    
    println("Sum: {}, Calc: {}", sum, calc);
}
